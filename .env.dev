# Sultan Prints Environment Variables
# تم إنشاؤه تلقائياً للتطوير المحلي

# إعدادات Flask
FLASK_APP=manage.py
FLASK_ENV=development
SECRET_KEY=dev-local-secret-key-very-secure-123

# قاعدة البيانات - تأكد من تعديل كلمة المرور إذا كان مطلوباً
DATABASE_URL=mysql+pymysql://root@localhost/sultan_prints?charset=utf8mb4

# إعدادات البريد الإلكتروني (يمكن تغييرها لاحقاً)
MAIL_SERVER=localhost
MAIL_PORT=25
MAIL_USERNAME=test
MAIL_PASSWORD=test
MAIL_DEFAULT_SENDER=test@example.com

# إعدادات المدير
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123

# مفتاح سري للإعداد
SETUP_SECRET_KEY=dev-setup-key-123
